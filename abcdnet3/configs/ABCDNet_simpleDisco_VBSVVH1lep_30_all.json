        "train_frac": 0.8,
        "n_batches_train": 10,
        "n_batches_test": 5,
        "learning_rate": 0.001,
        "seed": 1234,
        "scheduler_name": "ConstantLR",
        "scheduler_kwargs": {
            "factor": 1.0
        },
        "disco_lambda": 30,
        "disco_power": 1,
        "n_epochs": 1000
